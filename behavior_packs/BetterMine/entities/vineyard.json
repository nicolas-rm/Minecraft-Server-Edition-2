{
  "format_version": "1.16.0",
  "minecraft:entity": {
    "description": {
      "identifier": "better:vineyard",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {
      "better:loot_sheared": {
        "minecraft:loot": {
          "table": "loot_tables/entities/vineyard_sheared.json"
        }
      },
      "better:loot_grapey": {
        "minecraft:loot": {
          "table": "loot_tables/entities/vineyard_grapey.json"
        }
      },

      "better:vineyard_normal": {
        "minecraft:experience_reward": {
          "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
        },
        "minecraft:behavior.breed": {
          "priority": 3,
          "speed_multiplier": 1.0
        },
        "minecraft:breedable": {
          "require_tame": false,
          "breeds_with": {
            "mate_type": "minecraft:enderman",
            "baby_type": "minecraft:sheep"
          },
          "breed_items": "netherite_ingot"
        }
      },
      "better:vineyard_dyeable": {
        "minecraft:is_dyeable": {
          "interact_text": "action.interact.dye"
        }
      },
      "better:vineyard_sheared": {
        "minecraft:is_sheared": {
        }
      }
    },


    "components": {
      "minecraft:type_family": {
        "family": [ "vineyard", "mob" ]
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.9
      },
      "minecraft:health": {
        "value": 1,
        "max": 1
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": { "test": "in_lava", "subject": "self", "operator": "==", "value": true },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:movement": {
        "value": 0.0
      },
      "minecraft:navigation.walk": {
        "can_path_over_water": false,
        "avoid_water": false
      },
      "minecraft:interact": {
        "interactions": [
          {
            "cooldown": 2.5,
            "use_item": false,
            "hurt_item": 1,
            "spawn_items": { "table": "loot_tables/entities/vineyard_shear.json" },
            "play_sounds": "shear",
            "interact_text": "action.interact.shear",
            "on_interact": {
              "filters": {
                "all_of": [
                  { "test": "has_equipment", "subject": "other", "domain": "hand", "value": "shears"},
                  { "test" :  "is_family", "subject" : "other", "value" :  "player"}
                ]
              },
              "event": "minecraft:on_sheared",
              "target": "self"
            }
          }
        ]
      },
      "minecraft:balloonable": {
        "mass": 0.75
      },
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:behavior.panic": {
        "priority": 0,
        "speed_multiplier": 0.0
      },
      "minecraft:behavior.mount_pathing": {
        "priority": 0,
        "speed_multiplier": 0.0,
        "target_dist": 0.0,
        "track_target": false
      },
      "minecraft:behavior.tempt": {
        "priority": 0,
        "speed_multiplier": 0.0,
        "items": [
          "bedrock"
        ]
      },
      "minecraft:behavior.follow_parent": {
        "priority": 0,
        "speed_multiplier": 0.0
      },
      "minecraft:behavior.eat_block": {
        "priority": 6,
        "success_chance": "query.is_baby ? 0.02 : 0.001",
        "time_until_eat": 1.8,
        "eat_and_replace_block_pairs": [
          { "eat_block": "grass", "replace_block": "dirt" },
          { "eat_block": "tallgrass", "replace_block": "air" }
        ],
        "on_eat": {
          "event": "minecraft:on_eat_block",
          "target": "self"
        }
      },
      "minecraft:behavior.random_stroll": {
        "priority": 0,
        "speed_multiplier": 0.0
      },
      "minecraft:behavior.look_at_player": {
        "priority": 0,
        "look_distance": 0.0,
        "probability": 0.0
      },
      "minecraft:behavior.random_look_around": {
        "priority": 0
      },
      "minecraft:physics": {
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "sequence": [
          {
            "randomize": [
              {
                "weight": 95,
                "remove": {
                },
                "trigger": "spawn_normal"
              },
              {
                "weight": 5,
                "remove": {
                },
                "trigger": "spawn_normal"
              }
            ]
          },
          {
            "randomize": [
              {
                "weight": 81836,
                "add": {
                  "component_groups": [
                    "better:vineyard_normal"
                  ]
                }
              },
              {
                "weight": 5000,
                "add": {
                  "component_groups": [
                    "better:vineyard_normal"
                  ]
                }
              },
              {
                "weight": 5000,
                "add": {
                  "component_groups": [
                    "better:vineyard_normal"
                  ]
                }
              },
              {
                "weight": 5000,
                "add": {
                  "component_groups": [
                    "better:vineyard_normal"
                  ]
                }
              },
              {
                "weight": 3000,
                "add": {
                  "component_groups": [
                    "better:vineyard_normal"
                  ]
                }
              },
              {
                "weight": 164,
                "add": {
                  "component_groups": [
                    "better:vineyard_normal"
                  ]
                }
              }
            ]
          }
        ]
      },
      "spawn_normal": {
        "add": {
          "component_groups": [
            "better:vineyard_normal",
            "better:loot_grapey"
          ]
        }
      },
      "minecraft:on_sheared": {
        "remove": {
          "component_groups": [
            "better:loot_grapey"
          ]
        },
        "add": {
          "component_groups": [
            "better:vineyard_sheared",
            "better:loot_sheared"
          ]
        }
      },
      "minecraft:on_eat_block": {
        "sequence": [
          {
            "remove": {
              "component_groups": [
                "better:vineyard_sheared"
              ]
            },
            "add": {
              "component_groups": [
                "better:vineyard_dyeable"
              ]
            }
          },
          {
            "filters": {
              "test": "has_component",
              "subject": "other",
              "operator": "!=",
              "value": "minecraft:is_baby"
            },
            "add": {
              "component_groups": [
                "better:loot_grapey"
              ]
            },
            "remove": {
              "component_groups": [
                "better:loot_sheared"
              ]
            }
          }
        ]
      },
      "wololo": {
        "add": {
          "component_groups": [
            "minecraft:sheep_red"
          ]
        }
      }
    }
  }
}
