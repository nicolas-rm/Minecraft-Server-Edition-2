{
  "format_version": "1.13.0",
  "minecraft:feature_rules": {
    "description": {
      "identifier": "better:world_gen_apple_tree_feature",
      "places_feature": "better:apple_tree_feature"
    },
    "conditions": {
      "placement_pass": "surface_pass",
      "minecraft:biome_filter": [
        {
          "all_of": [
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "forest"
            },
            {
              "test": "has_biome_tag",
              "operator": "!=",
              "value": "canada"
            },
            {
              "test": "has_biome_tag",
              "operator": "!=",
              "value": "alps"
            },
            {
              "test": "has_biome_tag",
              "operator": "!=",
              "value": "birch"
            },
            {
              "test": "has_biome_tag",
              "operator": "!=",
              "value": "taiga"
            }
          ]
        }
      ]
    },
    "distribution": {
      "iterations": "math.clamp(math.trunc(math.ceil((query.noise(math.trunc(variable.originx / 20), math.trunc(variable.originz / 10)) + 0.2) * 10)), 2, 20)",
      "x": {
        "distribution": "uniform",
        "extent": [ 0, 16 ]
      },
      "y": {
        "distribution": "uniform",
        "extent": [ 0, "query.heightmap(variable.worldx, variable.worldz)" ]
      },
      "z": {
        "distribution": "uniform",
        "extent": [ 0, 16 ]
      }
    }
  }
}
