{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.gobble": {
      "states": {
        "default": {
          "transitions": [
            {
              "walking": "query.is_on_ground"
            }
          ]
        },
        "walking": {
          "transitions": [
            {
              "default": "query.is_on_ground"
            }
          ],
          "on_entry": [
           "/summon mythmob:goblin",
           "/summon mythmob:goblin.archer",
           "/summon mythmob:goblin.berserker"
          ]
        }
      }
    }
  }
}